{
  "name": "@simpleanalytics/react",
  "version": "0.0.1",
  "description": "Simple Analytics for React applications",
  "keywords": [
    "analytics",
    "simple analytics"
  ],
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "@types/react": "^19.0.12",
    "esbuild-plugin-preserve-directives": "^0.0.11",
    "prettier": "3.5.3",
    "tsup": "^8.4.0"
  },
  "peerDependencies": {
    "react": "^18 || ^19"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
